[{% for object in object_list %}
  {
    "id": {{ object.base_id }},
    "name": "{{ object.name }}",
    "hash": "{{ object.hash }}",
    "status": "{{ object.status }}",
    "total_size": "{{ object.fields.totalSize.value|filesizeformat }}",
    "left_size": "{{ object.fields.leftUntilDone.value|filesizeformat }}",
    "rate_download": "{{ object.fields.rateDownload.value|filesizeformat }}/s",
    "rate_upload": "{{ object.fields.rateUpload.value|filesizeformat }}/s",
    "message": "{{ object.fields.errorString.value }}",
    "progress": {{ object.progress }},
    "progress_css_class": "{{ object.progress_css_class }}",
    "date_added": "{{ object.date_added|date:'Y-m-d H:i' }}",
    "date_added_timesince": "{{ object.date_added|timesince }} ago"
  }{% if not forloop.last %},{% endif %}
{% endfor %}]
